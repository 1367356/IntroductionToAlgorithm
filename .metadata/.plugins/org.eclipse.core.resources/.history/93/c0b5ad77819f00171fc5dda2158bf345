package binarytree;

public class BinaryTree<T extends Comparable<T>> {
				Node<T> root=new Node<T>();
				 Node p=new Node();
				public class Node<T extends Comparable<T>>{
					Node leftChild;
					Node rightChild;
					Node parent;
					T key;
				}
				
				public void print(Node node){
					System.out.println(node.key);
				}
				
			   public void insert(T key){
				   Node newNode=new Node();
				   newNode.key=key;
				   Node tem=root;
				   if(root==null){
					   root=newNode;
					   root.leftChild=null;
					   root.rightChild=null;
				   }else{
					   while(tem!=null){
						  
						   p=tem;
						   if (key.compareTo(key)<0){
						   tem=tem.leftChild;
					        }
						   else{
							   tem=tem.rightChild;
						   }
						   }
				   }
				   newNode.parent=p;
				   tem=newNode;
				   
				   tem.leftChild=null;
				   tem.rightChild=null;
			   }
			  
}
