package binarytree;

public class BinaryTree<T extends Comparable<T>> {
				Node<T> root=null;
				 Node p=new Node();
				public class Node<T extends Comparable<T>>{
					Node leftChild;
					Node rightChild;
					Node parent;
					T key;
				}
				
				public void print(Node node){
					System.out.println(node.rightChild.key);
				}
				
			   public void insert(T key){
				   Node newNode=new Node();
				   newNode.key=key;
				   Node tem=root;
				   if(root==null){
					   root=newNode;
					   root.leftChild=null;
					   root.rightChild=null;
					   System.out.println(key);
				   }else{

					   while(tem!=null){
						  
						   p=tem;
						   if (key.compareTo(key)<0){
						   tem=tem.leftChild;
					        }
						   else{
							   tem=tem.rightChild;
						   }
						   }
					   tem=newNode;
					   tem.parent=p;
					   tem.leftChild=null;
					   tem.rightChild=null;
				   }

			   }
			  
}
