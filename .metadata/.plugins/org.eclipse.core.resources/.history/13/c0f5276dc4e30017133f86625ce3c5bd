package chapter19.ten;

public class CarWash {

	public enum Cycle{
		UNDERBODY{
			void action(){
				System.out.println("Spraying the underbody");
			}
		},
		WHEELWASH{
			void action(){
				System.out.println("Washing the wheels");
			}
		},
		PREWASH{
			void action(){
				System.out.println("Loosening the dirt");
			}
		}
	}
}
