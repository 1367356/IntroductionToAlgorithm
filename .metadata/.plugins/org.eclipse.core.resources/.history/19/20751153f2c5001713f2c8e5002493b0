package quicksort;

public class QuickSort {

	public void quickSort(int[] arr,int low,int high){
		if(low<high){
			int q=partition(arr,low,high);
			quickSort(arr, low, q);
			quickSort(arr, q+1, high);
		}
	}

	private int partition(int[] arr, int low, int high) {
		int x=arr[high];
		int i=low-1;
		return 0;
	}
}
