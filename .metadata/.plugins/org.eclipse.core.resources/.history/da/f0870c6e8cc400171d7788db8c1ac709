package mergesort;

public class MergeSort {

	public int Merge(int[] arr,int low,int high){
		int j=low;
		while(low<high){
			
		}
		return 1;
	}
	public int mergeSort(int[] arr,int low,int high){
		if(low<high){
			int mid=(low+high)/2;
			 Merge(arr, low, mid-1);
			 Merge(arr, mid+1, high);
			
		}

	}
	
}
